### ENV

export PATH "/usr/local/lib/python3.6:/usr/local/lib/python3.6/site-packages:/System/Library/Frameworks/Python.framework/Versions/2.7:$PATH:/Users/yuan/bin"


# syntax highlighting for cheat
export CHEATCOLORS true

# If you need to use these commands with their normal names, you
# can add a "gnubin" directory to your PATH from your bashrc like:
export PATH "/usr/local/opt/make/libexec/gnubin:$PATH"

# Additionally, you can access their man pages with normal names if you add
# the "gnuman" directory to your MANPATH from your bashrc as well:
export MANPATH "/usr/local/opt/make/libexec/gnuman:$MANPATH"

# rust
export PATH "$HOME/.cargo/bin:$PATH"

# pipenv shell now create .venv under project dir
export PIPENV_VENV_IN_PROJECT true

# tex support
export PATH "/Library/TeX/texbin/:$PATH"

# just print everthing and use Emacs as pager
export PAGER cat

### RC

# default editor
export VISUAL emacsclient
export EDITOR $VISUAL


# fix python error about locale
export LC_ALL en_US.UTF-8
export LANG en_US.UTF-8
